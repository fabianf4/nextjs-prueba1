{
    "extends": [
        "next/core-web-vitals",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "@typescript-eslin/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", 140],
        "quotes": [2, "single", { "avoidEscape": true }]
    }
}
